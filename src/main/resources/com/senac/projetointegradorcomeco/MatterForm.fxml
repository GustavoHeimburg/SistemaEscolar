<AnchorPane prefHeight="156.0" prefWidth="622.0" styleClass="root" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MatterFormController">
   <children>
      <GridPane layoutX="12.0" layoutY="4.0" prefHeight="100.0" prefWidth="317.0" AnchorPane.leftAnchor="16.0" AnchorPane.topAnchor="4.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="172.0" minWidth="10.0" prefWidth="108.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="333.0" minWidth="10.0" prefWidth="228.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="41.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="107.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Id" />
            <Label prefHeight="17.0" prefWidth="111.0" text="Nome da materia" GridPane.rowIndex="1" />
            <TextField fx:id="txtId" editable="false" GridPane.columnIndex="1" />
            <TextField fx:id="txtName" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="btCancel" layoutX="191.0" layoutY="114.0" mnemonicParsing="false" onAction="#onBtCancelAction" prefHeight="25.0" prefWidth="142.0" text="Cancelar" textFill="RED">
